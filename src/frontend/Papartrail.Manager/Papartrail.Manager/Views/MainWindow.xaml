<hc:Window
    x:Class="Papartrail.Manager.Views.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:consts="clr-namespace:Papartrail.Manager.Model.Consts"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:prism="http://prismlibrary.com/"
    Width="800"
    Height="550"
    AllowsTransparency="True"
    Background="Transparent"
    BorderThickness="0"
    ResizeMode="NoResize"
    WindowStartupLocation="CenterScreen"
    WindowStyle="None">

    <Grid>
        <ContentControl prism:RegionManager.RegionName="{x:Static consts:RegionNames.ContentRegion}" />
    </Grid>

</hc:Window>
